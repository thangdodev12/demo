{"ast":null,"code":"import { takeEvery, call, put } from \"redux-saga/effects\";\nimport { getInfo } from \"./actions\";\nimport { actionTypes } from \"./constants\";\nexport function* getInfoFlow() {\n  yield put(getInfo([]));\n}\nexport function* flow(action) {\n  switch (action.type) {\n    case actionTypes.GET_INFO:\n      yield call(getInfoFlow, action);\n      break;\n\n    default:\n      break;\n  }\n}\n\nfunction* sampleWatcher() {\n  yield takeEvery([], flow);\n}\n\nexport default sampleWatcher;","map":{"version":3,"sources":["/Users/Thang.DoX/Desktop/nextjs-ts-antd-saga/src/screens/Login/redux/saga.ts"],"names":["takeEvery","call","put","getInfo","actionTypes","getInfoFlow","flow","action","type","GET_INFO","sampleWatcher"],"mappings":"AAAA,SAASA,SAAT,EAAoBC,IAApB,EAA0BC,GAA1B,QAAqC,oBAArC;AAGA,SAASC,OAAT;AACA,SAASC,WAAT;AAEA,OAAO,UAAUC,WAAV,GAAmC;AACxC,QAAMH,GAAG,CAACC,OAAO,CAAC,EAAD,CAAR,CAAT;AACD;AAED,OAAO,UAAUG,IAAV,CAAeC,MAAf,EAA0C;AAC/C,UAAQA,MAAM,CAACC,IAAf;AACE,SAAKJ,WAAW,CAACK,QAAjB;AACE,YAAMR,IAAI,CAACI,WAAD,EAAcE,MAAd,CAAV;AACA;;AACF;AACE;AALJ;AAOD;;AAED,UAAUG,aAAV,GAAqC;AACnC,QAAMV,SAAS,CAAC,EAAD,EAAKM,IAAL,CAAf;AACD;;AAED,eAAeI,aAAf","sourcesContent":["import { takeEvery, call, put } from \"redux-saga/effects\";\n\nimport { Action } from \"../types\";\nimport { getInfo } from \"./actions\";\nimport { actionTypes } from \"./constants\";\n\nexport function* getInfoFlow(): Generator {\n  yield put(getInfo([]));\n}\n\nexport function* flow(action: Action): Generator {\n  switch (action.type) {\n    case actionTypes.GET_INFO:\n      yield call(getInfoFlow, action);\n      break;\n    default:\n      break;\n  }\n}\n\nfunction* sampleWatcher(): Generator {\n  yield takeEvery([], flow);\n}\n\nexport default sampleWatcher;\n"]},"metadata":{},"sourceType":"module"}